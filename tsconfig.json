{
  "compilerOptions": {
    "target":"ES5",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "outDir": "dist",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "./src",
    "experimentalDecorators":true,
    "noUnusedParameters":true
  },
}
